<script type="text/javascript">
	
	$(document).ready(function () {
		$.ajaxSetup({ cache: false });

        $.ajax({
            type: "GET",
            url: '@Url.Action("Index", "Dashboard")',
            contentType: "application/json; charset=utf-8",
            data: {},
            success: function (data) {
                $("#HtmlBody").html(data);
            }
        });
	})

</script>